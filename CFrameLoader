-- Scripts:

local function NYCZE_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local plr = game.Players.LocalPlayer
	local Humanoid = plr.Character:WaitForChild("HumanoidRootPart")
	
	while wait do
		task.wait(0.1)
		script.Parent.Text = "CFrame: ".. tostring(Humanoid.CFrame.Position)
		task.wait(0.1)
		script.Parent.Text = "CFrame: ".. tostring(Humanoid.CFrame.Position)
	end
	
	
end
coroutine.wrap(NYCZE_fake_script)()
