	script.Parent.MouseButton1Click:Connect(function()
	           
	end)
end
coroutine.wrap(XZYPBVC_fake_script)()
local function AWCHP_fake_script() -- Print.LocalScript 
	local script = Instance.new('LocalScript', Print)

	local plr = game.Players.LocalPlayer
	local Humanoid = plr.Character:WaitForChild("HumanoidRootPart")
	
	script.Parent.MouseButton1Click:Connect(function()
		print(tostring(Humanoid.CFrame.Position))
	end)
	
end
coroutine.wrap(AWCHP_fake_script)()
local function IDSM_fake_script() -- Credit.LocalScript 
	local script = Instance.new('LocalScript', Credit)

	while true do
	local text = "Made by: Vuzionix#8341"
	for i = 1, #text do
		script.Parent.Text = string.sub(text, 1, i)
		wait(0.05)
		end
		wait(3)
	end
end
coroutine.wrap(IDSM_fake_script)()
